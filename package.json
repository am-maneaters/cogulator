    "use-sound": "^4.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@biomejs/cli-darwin-x64": "^1.9.4",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vite-plugin-checker": "^0.8.0",
